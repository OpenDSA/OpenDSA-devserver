{% extends "base.html" %}

{% block breadcrumb %}
{% include "widgets/breadcrumb.html" with breadcrumb=exercise_round.get_breadcrumb %}
{% endblock %}

{% block content %} 
<h1>Exercise round {{ exercise_round.name }}</h1>
<ul>
    {% for ex in exercise_round.exercises.all %}
    <li>
        <a href="{{ ex.get_absolute_url }}">{{ ex.name }}</a>
    </li>
    {% endfor %}
</ul>
{% endblock %}